$('.btn').bind('ajax:success',function() {
  $(this).closest('div').find('span').html(<%= "#{@post.likes.count}" %>);
  $(this).closest('div').find('a').html("Liked");
  $(this).attr("href", "/posts/" + <%= "#{@post.id}" %>+"/likes/"+<%= "#{@like.id}" %>)
  $(this).attr("data-method","delete")

});
